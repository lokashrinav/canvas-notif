{
  "manifest_version": 3,
  "name": "Canvas Notifications",
  "version": "1.0",
  "description": "Get notifications for Canvas assignments.",
  "permissions": ["notifications", "storage", "alarms"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}